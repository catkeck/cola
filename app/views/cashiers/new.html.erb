<%= form_for(@cashier, url: cashiers_new_path) do |f| %>
  Name: <%= f.text_field :name %></br>
  Username: <%= f.text_field :username %></br>
  Password: <%= f.password_field :password%></br>
  Password Confirmation: <%= f.password_field :password_confirmation%></br>
  Store: <%= f.select :store_id, current_user.stores.collect { |store| [store.name, store.id]} %>
  <%= f.submit "Create New Cashier" %>
<% end %>